{
  "name": "sln",
  "version": "1.0.9",
  "description": "An npm local depedency manager",
  "scripts": {
    "postinstall": "npm run build",
    "build:compile": "tsc || exit 0",
    "build": "npm run build:compile",
    "test": "mocha 'dist/test/**/*.test.js' --reporter $npm_package_config_reporter",
    "test:watch": "npm run test -- --watch --bail"
  },
  "config": {
    "reporter": "spec"
  },
  "bin": {
    "sln": "bin/index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "async": "^1.5.2",
    "async-q": "^0.3.1",
    "charm": "^1.0.0",
    "child-process-promise": "^1.1.0",
    "command-line-args": "^2.1.6",
    "console.table": "^0.4.0",
    "convict": "^1.1.3",
    "fs": "0.0.2",
    "inquirer": "^0.12.0",
    "node-emoji": "^1.2.1",
    "nodegit": "^0.11.2",
    "npm": "^3.7.3",
    "path": "^0.12.7",
    "semver": "^5.1.0",
    "tsort": "0.0.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "sinon": "^1.17.3",
    "tsc": "^1.20150623.0",
    "typings": "^0.6.8"
  }
}
